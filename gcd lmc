#include <iostream>

using namespace std;

int main()
{
    int number1,number2,yndhanurBajanarar,yndhanurBazmapatik ;
    std::cout << "Enter number1 and number2" << std::endl;
    std::cin >> number1>>number2;

    for (int i = 1; i <= number1 && i<= number2; i++) {
        if(number1 % i == 0 && number2 % i == 0){
            yndhanurBajanarar=i;
        }
    }
    yndhanurBazmapatik = (number1 * number2) / yndhanurBajanarar;
    std::cout << "Amenamec yndhanur bajanarar e " <<yndhanurBajanarar << std::endl;
    std::cout << "Amenapoqr yndhanur bazmapatikn e " <<yndhanurBazmapatik << std::endl;

    return 0;
}
