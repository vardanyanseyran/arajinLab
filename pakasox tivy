#include<iostream>

using namespace std;

int main() {
   int n, s1 = 1, s2 = 1, res;
   cin >> n;
   int x[n];

   for(int i = 0; i < n; i++){
       cin >> x[i];
   }

   i = 0;
   while(i < n){
       s1 *= x[i];
       i++;
   }

   i = 1;
   while(i <= n + 1){
       s2 *= i;
       i++;
   }

   res= str2 / str1;

   cout << "Pakasox tivy " << res;

}
